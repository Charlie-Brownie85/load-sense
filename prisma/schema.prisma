generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Session {
  id        Int      @id @default(autoincrement())
  date      DateTime
  type      String
  duration  Int
  rpe       Int
  notes     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
